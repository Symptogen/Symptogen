<?xml version="1.0" encoding="utf-8"?>
<animation>
	<!-- frames declaration -->
	<frames>	
		<frame name="Rabbit1" file="rabbit.png" collision="rabbit_collision.xml"/>
		<frame name="Rabbit2" file="rabbit2.png" collision="rabbit_collision.xml"/>
	</frames>
		
	<!-- sequences declaration -->
	<sequences>
		<sequence name="rabbit_flash_normal">
			<frame name="Rabbit1" time="300" />
			<frame name="Rabbit2" time="300" />
		</sequence>
		<sequence name="rabbit_flash_fast">
			<frame name="Rabbit1" time="100" />
			<frame name="Rabbit2" time="100" />
		</sequence>
	</sequences>
</animation>